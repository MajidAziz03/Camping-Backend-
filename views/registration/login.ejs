<% layout('layout/boilerplate')  %> 
<h1 class="text-center mb-4 mt-4"> Login your Account </h1>


    <form action="/login" method="post">
        <input class="form-control mb-3 mt-3 " type="text" name="username" placeholder="Enter your username" required>
        <input class="form-control mb-3" type="password" name="password" placeholder="Enter your password" required>
        <button class="btn btn-success mb-3 " > Login </button>
        <p> Don't have an Account <a href="/register"> Create a new one </a></p>
    </form>

